<view class="container">
  <image class="character" src="/static/images/1.png" />
  <text class="title">我的问答</text>
  <view class="profile-card">
    <image class="avatar" src="{{}}" />
    <view class="profile-info">
      <view class="name">{{userData.username}}</view>
      <view class="group">{{userData.groupName}}</view>
    </view>
    <view class="score">
      <text class="score-value">{{userData.score}}</text>
      <text class="score-label">积分</text>
    </view>
    <text class="score-ranking" bindtap="gotoRanking">积分排行榜 ></text>
  </view>
